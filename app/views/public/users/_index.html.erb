<% @posts.each do |post| %>
  <%= post.user.get_profile_image(50,50) %><br>
  <%= post.user.first_name %><%= post.user.lat_name %><br>
  <%= post.get_post_image(300,300) %>
  <%= post.caption %>
  <%= post.post_category %>
  <%= post.color_type %>
  <%= post.makeup_image %>
  <%= post.all_records[:main_category_id].main_category%>
  <%= post.all_records[:sub_category_id].sub_category%>
<% end %>