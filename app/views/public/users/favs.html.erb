<div class="container">
  <div class="row">
    <div class="col-md-10">
      <% @fav_posts.each do |post|%>
        <%= link_to post_path(post) do %>
          <% post.post_images.each do |post_image| %>
            <%= image_tag post_image.variant(resize_to_limit: [240,240]) %>
          <% end %>
        <% end %>
      <% end %>
    </div>
  </div>
</div>