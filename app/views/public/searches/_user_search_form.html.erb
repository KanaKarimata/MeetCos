<% if user_signed_in? %>
  <%= form_with url: search_path, method: :get, local: true do |f| %>
    <%= f.text_field :content %>
    <%#= f.select :model, options_for_select({"User" => "user", "Book" => "book", "Hashtag" => "hashtag"})%>
    <%#= f.select :method, options_for_select({"完全一致" => "perfect", "前方一致" => "forward", "後方一致" => "backword", "部分一致" => "partial" })%>
    <%= f.button type: "submit", class:"btn" do %>
      <i class="fas fa-search search_btn_icon"></i>
    <% end %>
  <% end %>
<% end %>